msgid "I really mean it!!!"
msgstr "Я именно это имею в виду!!!"

#: ../src/cairo-clock.c:875
#, c-format
msgid "Ups, there was an error while trying to save the preferences!\n"
msgstr "Ой, при попытке сохранить настройки возникла ошибка!\n"

#: ../src/cairo-clock.c:1330
msgid "x-position of the top-left window-corner"
msgstr "x-координата верхнего левого угла окна"

#: ../src/cairo-clock.c:1337
msgid "y-position of the top-left window-corner"
msgstr "y-координата верхнего левого угла окна"

#: ../src/cairo-clock.c:1344
msgid "open window with this width"
msgstr "открыть окно такой ширины"

#: ../src/cairo-clock.c:1351
msgid "open window with this height"
msgstr "открыть окно такой высоты"

#: ../src/cairo-clock.c:1358
msgid "draw seconds hand"
msgstr "рисовать секундную стрелку"

#: ../src/cairo-clock.c:1365
msgid "draw date-display"
msgstr "показывать текущую дату"

#: ../src/cairo-clock.c:1372
msgid "list installed themes and exit"
msgstr "перечислить установленные темы и выйти"

#: ../src/cairo-clock.c:1379
msgid "theme to draw the clock with"
msgstr "тема для часов"

#: ../src/cairo-clock.c:1386
msgid "clock-window stays on top of all windows"
msgstr "окно часов остаётся поверх всех окон"

#: ../src/cairo-clock.c:1393
msgid "clock-window shows up in pager"
msgstr "окно часов отображается в пейджере"

#: ../src/cairo-clock.c:1400
msgid "clock-window shows up in taskbar"
msgstr "окно часов отображается на панели задач"

#: ../src/cairo-clock.c:1407
msgid "clock-window sticks to all workspaces"
msgstr "окно часов отображается на всех рабочих столах"

#: ../src/cairo-clock.c:1414
msgid "hands work in 12 hour mode"
msgstr "стрелки работают в 12 часовом режиме"

#: ../src/cairo-clock.c:1421
msgid "hands work in 24 hour mode"
msgstr "стрелки работают в 24 часовом режиме"

#: ../src/cairo-clock.c:1428
msgid "render at RATE (default: 30 Hz)"
msgstr "перерисовывать с указанной скоростью RATE (по умолчанию: 30 Hz)"

#: ../src/cairo-clock.c:1435
msgid "print version of program and exit"
msgstr "напечатать номер версии программы и выйти"

