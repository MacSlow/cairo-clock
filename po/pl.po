#: ../glade/cairo-clock.glade.h:287
msgid "Keep _on top"
msgstr "Zawsze na wierzchu"

#: ../glade/cairo-clock.glade.h:289
msgid "MacSlow's Cairo-Clock - Error"
msgstr "Błąd w zegarze Cairo MacSlowa"

#: ../glade/cairo-clock.glade.h:290
msgid "MacSlow's Cairo-Clock - Information"
msgstr "Informacje o zegarze Cairo MacSlowa"

#: ../glade/cairo-clock.glade.h:291
msgid "MacSlow's Cairo-Clock - Settings"
msgstr "Ustawienia zegara Cairo MacSlowa"

#: ../glade/cairo-clock.glade.h:292
msgid "Settings"
msgstr "Preferencje"

#: ../glade/cairo-clock.glade.h:293
msgid "Quit"
msgstr "Wyjście"

#: ../glade/cairo-clock.glade.h:294
msgid "Show _date"
msgstr "Pokaż _datę"

#: ../glade/cairo-clock.glade.h:295
msgid "Show _seconds"
msgstr "Pokaż _sekundy"

#: ../glade/cairo-clock.glade.h:296
msgid "Stic_k to every workspace"
msgstr "Na wszystkich pulpitach"

#: ../glade/cairo-clock.glade.h:297
msgid "_Use 24h mode"
msgstr "_Użyj trybu 24-godzinnego"

#: ../glade/cairo-clock.glade.h:298
msgid "_Width:"
msgstr "Szerokość:"

#: ../glade/cairo-clock.glade.h:299
msgid "an analog clock based on vector-graphics"
msgstr "analogowy zegar oparty na grafice wektorowej"

#: ../glade/cairo-clock.glade.h:300
msgid "cairo-clock Homepage"
msgstr "strona domowa cairo-clock"

#: ../glade/cairo-clock.glade.h:301
msgid "close this settings-dialog"
msgstr "zamknij ten dialog ustawień"

#: ../glade/cairo-clock.glade.h:302
msgid "get information on all settings-options"
msgstr "informacje o wszystkich opcjach"

#: ../glade/cairo-clock.glade.h:303
msgid "if enabled the workspace pager will show the window of the clock"
msgstr "jeśli włączone lista zadań pokaże okno zegara"

#: ../glade/cairo-clock.glade.h:304
msgid "if enabled, the clock window will be kept above all other windows"
msgstr "jeśli włączone, okno zegara będzie zawsze powyżej innych okien"

#: ../glade/cairo-clock.glade.h:305
msgid "if enabled, the current date will be displayed inside the clocks face"
msgstr "jeśli włączone, bieżąca data zostanie wyświetlona na tarczy zegara"

#: ../glade/cairo-clock.glade.h:306
msgid "if enabled, the seconds hand will be drawn"
msgstr "jeśli włączone, pojawi się wskazówka sekund"

#: ../glade/cairo-clock.glade.h:307
msgid "if enabled, the window of the clock will be shown in the taskbar"
msgstr "jeśli włączone, tacka systemowa pokaże okno zegara"

#: ../glade/cairo-clock.glade.h:308
msgid "initial window-height when program starts"
msgstr "wstępna wysokość okna po starcie programu"

#: ../glade/cairo-clock.glade.h:309
msgid "initial window-width when program starts"
msgstr "wstępna szerokość okna po starcie programu"

#: ../glade/cairo-clock.glade.h:310
msgid "rough"
msgstr "ostry"

#: ../glade/cairo-clock.glade.h:311
msgid ""
"small (50x50)\n"
"medium (100x100)\n"
"large (200x200)\n"
"custom"
msgstr ""
"mały (50x50)\n"
"średni (100x100)\n"
"duży (200x200)\n"
"inny"

